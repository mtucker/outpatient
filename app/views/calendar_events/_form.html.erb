<%= simple_form_for(@calendar_event) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :user %>
    <%= f.input :type %>
    <%= f.input :starts_at %>
    <%= f.input :ends_at %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
